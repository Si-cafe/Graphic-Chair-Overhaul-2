<?xml version="1.0" encoding="utf-8"?>
<Patch>
<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Factions Expanded - Empire</li>
    </mods>
    <match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
<!-- VFEE_GrandChair -->
		<li Class="PatchOperationConditional">
		<xpath>Defs/ThingDef[defName = "VFEE_GrandChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName = "VFEE_GrandChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>	
        <li Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName = "VFEE_GrandChair"]/comps</xpath>
			<value>
				<li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
					<graphicData/>
					<allwaysShowGizmo>true</allwaysShowGizmo>
				</li>
			</value>
		</li>
		<li Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName = "VFEE_GrandChair"]</xpath>
		<value>
			<drawerType>MapMeshAndRealTime</drawerType>
		</value>
		</li>
<!-- VFEE_StellicThrone -->
		<li Class="PatchOperationConditional">
		<xpath>Defs/ThingDef[defName = "VFEE_StellicThrone"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName = "VFEE_StellicThrone"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>	
        <li Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName = "VFEE_StellicThrone"]/comps</xpath>
			<value>
				<li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
					<graphicData/>
					<allwaysShowGizmo>true</allwaysShowGizmo>
				</li>
			</value>
		</li>
		<li Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName = "VFEE_StellicThrone"]</xpath>
		<value>
			<drawerType>MapMeshAndRealTime</drawerType>
		</value>
		</li>
	</operations>
    </match>
</Operation>

</Patch>
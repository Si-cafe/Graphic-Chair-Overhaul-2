<?xml version="1.0" encoding="utf-8"?>
<Patch>
<Operation Class="PatchOperationFindMod">
    <mods>
		<li>More Furniture (Continued)</li>
    </mods>
    <match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
<!-- Anon2SquareDiningChair -->
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Anon2SquareDiningChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Anon2SquareDiningChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>
		<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2SquareDiningChair"]/comps</xpath>
			<value>
				<li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
					<graphicData/>
					<altitudeLayer>PawnState</altitudeLayer>
					<allwaysShowGizmo>false</allwaysShowGizmo>
				</li>
			</value>
		</li>
<!-- Anon2CushionedChair -->
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Anon2CushionedChair"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Anon2CushionedChair"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>	
		<li  Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2CushionedChair"]/comps</xpath>
			<value>
				<li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
					<graphicData/>
					<altitudeLayer>PawnState</altitudeLayer>
					<allwaysShowGizmo>false</allwaysShowGizmo>
				</li>
			</value>
		</li>
<!-- Anon2CouchRight -->
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Anon2CouchRight"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Anon2CouchRight"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>
		<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2CouchRight"]/comps</xpath>
			<value>
				<li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
					<graphicData/>
					<altitudeLayer>PawnState</altitudeLayer>
					<allwaysShowGizmo>false</allwaysShowGizmo>
				</li>
			</value>
		</li>
<!-- Anon2CouchLeft -->
		<li Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName = "Anon2CouchLeft"]/comps</xpath>
		<success>Always</success>
		<nomatch Class="PatchOperationAdd">
			<xpath>/Defs/ThingDef[defName = "Anon2CouchLeft"]</xpath>
				<value>
					<comps/>
				</value>
		</nomatch>
		</li>
		<li Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName = "Anon2CouchLeft"]/comps</xpath>
			<value>

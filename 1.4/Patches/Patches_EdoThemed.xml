<?xml version="1.0" encoding="utf-8"?>
<Patch>
<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Edo Themed Expansion</li>
    </mods>
	<match Class="PatchOperationFindMod">
	<mods>
		<li>Royalty</li>
	</mods>
	<match Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
<!-- HSEdoThrone -->
		<li Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName = "HSEdoThrone"]/comps</xpath>
			<value>
				<li Class="DoubleGraphicLayer.CompProperties_SecondLayer">
					<graphicData/>
					<allwaysShowGizmo>false</allwaysShowGizmo>
				</li>
			</value>
		</li>
		<li Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName = "HSEdoThrone"]</xpath>
			<value>
				<drawerType>MapMeshAndRealTime</drawerType>
			</value>
		</li>

	</operations>
    </match>
    </match>	
</Operation>

</Patch>